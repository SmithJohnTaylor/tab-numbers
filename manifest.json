{
  "manifest_version": 2,
  "name": "JT Numbered Tabs",
  "description": "Tab numbering for easy navigation",
  "version": "0.1",
  "permissions": [
    "file://*/*",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "toggle_title": {
      "suggested_key": {
        "default": "Alt+T"
      },
      "description": "Triggers title change"
    }
  }
}